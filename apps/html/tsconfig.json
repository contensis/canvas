{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "lib": [
      "ESNext",
      "DOM"
    ],
    "moduleResolution": "Node",
    "strict": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "noEmit": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "skipLibCheck": true,
    "jsx": "react",
    "jsxFactory": "h",
    "jsxFragmentFactory": "h.fragment",
    "paths": {
      "@contensis-canvas/types": [
        "../../packages/types/src"
      ],
      "@contensis-canvas/dom": [
        "../../packages/dom/src"
      ],
      "@contensis-canvas/html": [
        "../../packages/html/src"
      ],
      "@contensis-canvas/html-dom": [
        "../../packages/html-dom/src"
      ]
    }
  },
  "include": [
    "src"
  ]
}