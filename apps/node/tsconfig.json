{
	"compilerOptions": {
		"outDir": "./dist",
		"target": "es2016",
		"module": "commonjs",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true,
		"jsx": "react",
		"jsxFactory": "h",
		"jsxFragmentFactory": "h.fragment",
		"paths": {
			"@contensis-canvas/types": [
			  "../../packages/types/src"
			],
			"@contensis-canvas/dom": [
			  "../../packages/dom/src"
			],
			"@contensis-canvas/html": [
			  "../../packages/html/src"
			]
		}
	}
}